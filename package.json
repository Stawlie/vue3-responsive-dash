{
  "name": "vue3-responsive-dash",
  "version": "0.1.4",
  "keywords": [
    "vuejs",
    "vue",
    "vue3",
    "responsive",
    "drag",
    "draggable",
    "resize",
    "resizable",
    "dashboard",
    "grid",
    "grid-layout",
    "components"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Stawlie/vue3-responsive-dash.git"
  },
  "files": [
    "dist",
    "types/*.d.ts"
  ],
  "types": "./types/index.d.ts",
  "author": "Stawlie",
  "main": "dist/vue3-responsive-dash.common.js",
  "unpkg": "dist/vue3-responsive-dash.umd.min.js",
  "scripts": {
